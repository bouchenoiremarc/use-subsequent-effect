  "description": "A use(Layout)Effect variant hook that skips the initial render.",
  "author": "Marc Bouchenoire",
  "license": "MIT",
  "repository": "https://github.com/bouchenoiremarc/use-subsequent-effect",
  "keywords": [
    "react",
    "hook",
    "subsequent",
    "initial",
    "effect"
  ],
  "files": [
    "dist",
    "src"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/use-subsequent-effect.esm.js",
  "typings": "dist/use-subsequent-effect.d.ts",
  "scripts": {
